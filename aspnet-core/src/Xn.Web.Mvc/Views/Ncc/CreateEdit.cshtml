@*ng-show="ifShow"*@
<form role="form" name="form" ng-submit="onSubmit(0)">
    <div id="myModal" class="modal fade modal-createedit" role="dialog">

        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">{{infoEvent}}</h4>
                </div>
                <div class="modal-body">
                    <div class="create-edit">

                        <div class="col-sm-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    <input class="form-control" type="text" ng-model="TenNcc"  ng-blur="keypressNcc()" name="TenNcc" required="" maxlength="32" minlength="2" aria-required="true">
                                    <label class="form-label">Tên Công Ty <i style="color: red">{{warring}}</i></label>
                                </div>
                            </div>
                           
                        </div>
                        <div class="col-sm-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    <input class="form-control" ng-model="MasoThue" type="text" name="MasoThue">
                                    <label class="form-label">Mã Số Thuế</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    <input class="form-control" type="text" ng-model="DienThoai" name="DienThoai" required="" maxlength="32" minlength="2" aria-required="true">
                                    <label class="form-label">Điện Thoại</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    <input class="form-control" type="text" ng-model="DiaChi" name="DiaChi" required="" maxlength="32" minlength="2" aria-required="true">
                                    <label class="form-label">Địa Chỉ</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    <input class="form-control" type="text" ng-model="DanhMucHang" name="DanhMucHang" required="" maxlength="32" minlength="2" aria-required="true">
                                    <label class="form-label">Danh Mục hàng</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    <input class="form-control" type="date" ng-model="NgayGhi" value="@DateTime.Today" name="NgayGhi" required="" maxlength="32" minlength="2" aria-required="true">
                                    <label class="form-label">Ngày Ghi</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    <input class="form-control" type="text" ng-model="TenNv" name="TenNv" required="" maxlength="32" minlength="2" aria-required="true">
                                    <label class="form-label">Nhân Viên Ghi </label>
                                </div>
                            </div>
                        </div>

                        <div class="hidden">
                            <input type="text" name="Loai" value="{{Loai}}" />
                            <input name="Id" ng-model="Id" value="{{id}}" />
                            @*<input ng-model="Id" name="Id" value="0" />*@
                            <input ng-model="IdCty" name="IdCty" value="{{idcty}}" />
                            <input ng-value="idcty" />
                        </div>
                        

                    </div>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary waves-effect">Submit</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>

        </div>
    </div>
</form>
